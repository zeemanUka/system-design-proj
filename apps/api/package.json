{
  "name": "@sdc/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "prisma:generate": "cd ../.. && prisma generate --schema apps/api/prisma/schema.prisma",
    "prisma:validate": "cd ../.. && prisma validate --schema apps/api/prisma/schema.prisma",
    "prisma:db:push": "cd ../.. && prisma db push --schema apps/api/prisma/schema.prisma",
    "prisma:migrate": "cd ../.. && prisma migrate dev --schema apps/api/prisma/schema.prisma",
    "prisma:seed": "cd ../.. && tsx apps/api/prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.0",
    "@nestjs/core": "^11.1.0",
    "@nestjs/platform-fastify": "^11.1.0",
    "@prisma/client": "^6.5.0",
    "@sdc/shared-types": "0.1.0",
    "@sdc/simulation-core": "0.1.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.44.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.15.1",
    "fastify": "^5.2.1",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.10",
    "prisma": "^6.5.0",
    "tsx": "^4.19.3",
    "vitest": "^3.0.9"
  }
}
